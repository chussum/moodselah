{
  "name": "moodselah-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://127.0.0.1:4000",
  "scripts": {
    "start": "node scripts/start.js",
    "start:dev": "node scripts/start.js",
    "build": "sh ./build.sh",
    "test": "node scripts/test.js",
    "tcm": "tcm src",
    "tsm": "node scripts/tsm src",
    "precodegen": "apollo schema:download --endpoint=http://127.0.0.1:4000/graphql",
    "codegen": "apollo codegen:generate src/types/api.d.ts --queries='src/**/!(*.local).ts' --addTypename --localSchemaFile schema.json --target typescript --outputFlat",
    "lint:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --project tsconfig.json",
    "postinstall": "cd .. && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tslint.json --project tsconfig.json"
    ],
    "*.{js,jsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@babel/core": "7.1.6",
    "@material-ui/core": "3.9.2",
    "@svgr/webpack": "2.4.1",
    "@types/history": "4.7.2",
    "@types/jest": "23.3.13",
    "@types/js-cookie": "2.2.0",
    "@types/json5": "0.0.30",
    "@types/lodash-es": "4.17.1",
    "@types/node": "10.12.20",
    "@types/query-string": "6.2.0",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.0.11",
    "@types/react-helmet": "5.0.8",
    "@types/react-loadable": "5.5.0",
    "@types/react-router-dom": "4.3.1",
    "@types/socket.io-client": "1.4.32",
    "apollo": "2.5.3",
    "apollo-boost": "0.1.28",
    "apollo-cache-inmemory": "1.4.3",
    "apollo-client": "2.4.13",
    "apollo-link": "1.2.8",
    "apollo-link-error": "1.1.7",
    "apollo-link-http": "1.5.11",
    "apollo-link-state": "0.4.2",
    "apollo-link-ws": "1.0.14",
    "apollo-upload-client": "^10.0.0",
    "apollo-utilities": "1.1.3",
    "autobind-decorator": "2.4.0",
    "axios": "0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "0.3.1",
    "babel-preset-react-app": "7.0.1",
    "bfj": "6.1.1",
    "blueimp-load-image": "^2.20.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.14.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-react-app": "3.0.7",
    "eslint-loader": "2.1.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-module-resolver": "0.8.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "exif-js": "2.3.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "graphql": "14.1.1",
    "history": "4.7.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "i18next": "14.0.1",
    "identity-obj-proxy": "3.0.0",
    "immer": "1.12.1",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jquery": "3.3.1",
    "js-cookie": "2.2.0",
    "json5": "2.1.0",
    "lodash-es": "4.17.11",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "query-string": "6.2.0",
    "react": "16.8.2",
    "react-apollo": "2.4.1",
    "react-app-polyfill": "0.2.1",
    "react-dev-utils": "7.0.3",
    "react-dom": "16.8.2",
    "react-helmet": "5.2.0",
    "react-i18next": "9.0.10",
    "react-infinite-scroller": "^1.2.4",
    "react-loadable": "5.5.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-slick": "0.23.2",
    "react-toastify": "4.5.2",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "slick-carousel": "1.8.1",
    "style-loader": "0.23.0",
    "subscriptions-transport-ws": "0.9.15",
    "terser-webpack-plugin": "1.1.0",
    "ts-optchain": "0.1.2",
    "tslint": "5.12.1",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3",
    "typescript-tslint-plugin": "0.3.1",
    "url-loader": "1.1.1",
    "webpack": "4.29.4",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "babel-plugin-module-resolver": "3.2.0",
    "dotenv-webpack": "1.7.0",
    "eslint-import-resolver-babel-module": "5.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.1",
    "node-sass": "4.11.0",
    "postcss-pxtorem": "4.0.1",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "typed-css-modules": "0.3.7"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
