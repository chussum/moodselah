{
  "apps": [
    {
      "name": "moodselah-server",
      "script": "./src/index.ts",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "instances": 1,
      "error_file": "./logs/moodselah-web-err.log",
      "out_file": "./logs/moodselah-web-out.log",
      "pid_file": "./logs/moodselah-web.pid",
      "max_restarts": 10,
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "TS_NODE_PROJECT": "tsconfig.json",
        "TS_NODE_FILES": "true"
      }
    }
  ]
}
